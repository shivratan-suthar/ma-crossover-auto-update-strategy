    "def Best_ma(df,S_colms,L_colms,ma_diff,take_short):\n",
    "    L_list=[]\n",
    "    S_list=[]\n",
    "    rtn=[]\n",
    "    \n",
    "    for L in L_colms:\n",
    "        for S in S_colms:\n",
    "            if (int(L.strip(\"Ma\"))-int(S.strip(\"Ma\")))>=ma_diff:\n",
    "                L_list.append(L)\n",
    "                S_list.append(S)\n",
    "                rtn.append(Calc_rtn(df,S,L,short_pos=take_short))\n",
    "    return(S_list[rtn.index(max(rtn))],L_list[rtn.index(max(rtn))])"
   ]
  },
